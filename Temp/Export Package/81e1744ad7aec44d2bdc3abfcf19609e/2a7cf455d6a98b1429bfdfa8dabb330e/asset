namespace Geekbrains
{
	public class Ammunition : BaseObjectScene
	{
		public float _timeToDestuct = 15;
		public float _baseDamage = 15;
		protected float _currentDamage;

		protected override void Awake()
		{
			base.Awake();
			Destroy(gameObject, _timeToDestuct);
			_currentDamage = _baseDamage; // как будет меняться урон 
		}
	}
}