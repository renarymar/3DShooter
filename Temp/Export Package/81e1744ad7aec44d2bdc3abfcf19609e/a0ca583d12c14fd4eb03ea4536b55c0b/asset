using UnityEngine;

namespace Geekbrains
{
	public sealed class Test : MonoBehaviour
	{
		private void Start()
		{
			MyInvoke(ExampleInvoke, 3);
		}

		private void ExampleInvoke()
		{
			Debug.Log(1);
		}

		private void MyInvoke(System.Action method, float time)
		{ 
			Invoke(method.Method.Name, time);
		}
	}
}